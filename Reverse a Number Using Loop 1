import java.io.*;
import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        long a=s.nextLong();
        int rev=0;
        if(a>0 && a<1000000000){
            while(a>0){
                int rem=(int)a%10;
                rev=rem;
                System.out.print(rev);
                a=a/10;
            }
        }
        else{
            System.out.println("Invalid Input");
        }
    }
}
