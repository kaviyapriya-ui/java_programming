import java.io.*;
import java.util.*;
import java.lang.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        float a=s.nextFloat();
        int b=s.nextInt();
        int c=s.nextInt();
        int d=s.nextInt();
        System.out.printf("%d",(int)a);
        System.out.printf("\n%d",(int)Math.ceil(a));
        System.out.printf("\n%d",(int)Math.sqrt(b));
        System.out.printf("\n%d",(int)Math.pow(c,d));
    }
}
