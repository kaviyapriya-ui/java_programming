import java.io.*;
import java.util.*;

public class Solution {
      public static void main(String[] args) {
          Scanner s=new Scanner(System.in);
          int a=s.nextInt();
          int b=s.nextInt();
          char c=s.next().charAt(0);
          switch(c){
              case '+':float add=(float)a+b;
                  System.out.printf("Addition of two number is "+add);
                  break;
              case '-':float sub=(float)a-b;
                  System.out.println("Subtraction of two number is "+sub);
                  break;
              case '*':float mul=(float)a*b;
                  System.out.println("Multiplication of two number is "+mul);
                  break;
              case '/':float div=(float)a/b;
                  System.out.println("Division of two number is "+div);
                  break;
              case '%':float mod=(float)a%b;
                  System.out.println("Modulo of two number is "+mod);
                  break;
              default:System.out.println("Invalid Input");
          }
          
    }
}
