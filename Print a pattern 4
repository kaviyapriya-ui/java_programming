import java.io.*;
import java.util.*;
public class Solution {
  public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
    int a=s.nextInt();
    char ch='A';
    if(a>=0 && a<=9){
    for(int i=1;i<=a;i++){
        for(int j=1;j<=i;j++)
        {
            System.out.print(ch+" ");
            ch++;
        }
        System.out.println();
    }
    }
      else{
          System.out.println("Invalid Input");
      }
  }
}
